<IfModule mod_rewrite.c>

    RewriteEngine On

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteBase /

    RewriteRule ^([^/]+)/?$ index.php?url=$1 [QSA,L]
    RewriteRule ^([^/]+)/([^/]+)/?$ index.php?url=$1/$2 [QSA,L]

    ErrorDocument 404 /
</IfModule>


